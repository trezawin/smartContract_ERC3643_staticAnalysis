    "phase2:bootstrap": "node --require ts-node/register scripts/bootstrap-clean.ts",
    "phase2:run": "node scripts/phase2-check.js",
    "phase2:render": "node scripts/phase2-render.js",
    "phase2:open": "open reports/phase2-report.html",
    "p2": "npm run phase2:bootstrap && npm run phase2:run && npm run phase2:render && npm run phase2:open"
